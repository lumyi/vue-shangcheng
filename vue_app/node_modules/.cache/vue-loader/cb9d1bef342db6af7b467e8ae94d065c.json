ss='mui-ellipsis'>\r\n                            <span>{{item.ctime | timer}}</span>\r\n                            <span>点击 {{item.point}} 次</span>\r\n                        </p>\r\n                    </div>\r\n                </router-link>\r\n            </li>\r\n        </ul>\r\n        <!-- 加载更多 -->\r\n        <mt-button size=\"large\" type=\"primary\" @click=\"loadMore\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    data(){\r\n        return {\r\n            pno:0,\r\n            newslist:[]\r\n        }\r\n    },\r\n    methods:{\r\n        loadMore(){\r\n            this.pno++;\r\n            this.axios.get(\r\n                \"http://localhost:3000/getNews\",\r\n                {\r\n                    params:{\r\n                        pno:this.pno\r\n                    }\r\n                }\r\n            ).then((res)=>{\r\n                // console.log(res.data);\r\n                var rows=this.newslist.concat(res.data.data);\r\n                this.newslist=rows;\r\n            })\r\n        },\r\n    },\r\n    created(){\r\n        this.loadMore();\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n    .app-news .mui-ellipsis{\r\n        display:flex;\r\n        font-size:12px;\r\n        color:226aff;\r\n        justify-content:space-between;\r\n\r\n    }\r\n</style>"]}]}